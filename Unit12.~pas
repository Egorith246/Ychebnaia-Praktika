unit Unit12;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls, jpeg, ExtCtrls;

type
  TForm12 = class(TForm)
    img1: TImage;
    btn1: TButton;
    btn3: TButton;
    btn6: TButton;
    btn5: TButton;
    btn4: TButton;
    procedure btn1Click(Sender: TObject);
    procedure btn3Click(Sender: TObject);
    procedure btn5Click(Sender: TObject);
    procedure btn6Click(Sender: TObject);
    procedure btn4Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form12: TForm12;

implementation

uses
  Unit8, Unit11, Unit14, Unit15, Unit16, Unit18;

{$R *.dfm}

procedure TForm12.btn1Click(Sender: TObject);
begin
     Form11.Show;
end;

procedure TForm12.btn3Click(Sender: TObject);
begin
Form14.Show;
end;

procedure TForm12.btn5Click(Sender: TObject);
begin
Form15.Showmodal;
end;

procedure TForm12.btn6Click(Sender: TObject);
begin
   form16.showmodal;
end;

procedure TForm12.btn4Click(Sender: TObject);
begin
     form18.showmodal;
end;

end.
